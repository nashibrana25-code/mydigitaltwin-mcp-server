{
  "name": "digital-twin-mcp",
  "version": "1.0.0",
  "description": "Digital Twin MCP Server - RAG-powered professional assistant",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:all": "next build && pnpm build:server",
    "build:server": "tsc server/index.ts --outDir dist --module commonjs --target es2020 --moduleResolution node --esModuleInterop --skipLibCheck",
    "start": "next start",
    "start:server": "node start-server.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@upstash/vector": "^1.1.5",
    "@vercel/analytics": "^1.5.0",
    "clsx": "^2.1.0",
    "groq-sdk": "^0.7.0",
    "next": "^15.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.3",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0"
  },
  "packageManager": "pnpm@10.18.3"
