{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "UPSTASH_VECTOR_REST_URL": "@upstash_vector_url",
    "UPSTASH_VECTOR_REST_TOKEN": "@upstash_vector_token",
    "UPSTASH_VECTOR_REST_READONLY_TOKEN": "@upstash_vector_readonly_token",
    "GROQ_API_KEY": "@groq_api_key"
  }
}
